{
  "name": "Ganymede",
  "description": "Run Jupyter Lab as a native app",
  "version": "0.0.2",
  "main": "app/index.js",
  "repository": "dfm/Ganymede",
  "author": "Dan Foreman-Mackey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dfm/Ganymede/issues"
  },
  "homepage": "https://github.com/dfm/Ganymede",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && electron ./app",
    "pack": "yarn build && electron-builder --dir",
    "dist": "yarn build && electron-builder",
    "locate": "yarn build && node ./app/locate.js"
  },
  "build": {
    "appId": "io.dfm.Ganymede",
    "productName": "Ganymede",
    "publish": [
      {
        "provider": "github"
      }
    ],
    "fileAssociations": {
      "ext": "ipynb",
      "name": "ipynb"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "files": [
      "app/*.js"
    ],
    "extraResources": [
      "ganymede.sh"
    ]
  },
  "dependencies": {
    "electron-prompt": "^1.3.1",
    "electron-settings": "^3.2.0",
    "glob": "^7.1.4",
    "untildify": "^4.0.0",
    "which": "^1.3.1",
    "yargs": "^13.3.0"
  },
  "devDependencies": {
    "electron": "^5.0.7",
    "electron-builder": "^21.1.1",
    "typescript": "^3.5.3"
  }
}
