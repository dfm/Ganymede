{
  "name": "ganymede",
  "description": "Run Jupyter Lab as a native app",
  "version": "0.0.2",
  "main": "app/index.js",
  "repository": "dfm/Ganymede",
  "author": "Dan Foreman-Mackey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dfm/Ganymede/issues"
  },
  "homepage": "https://github.com/dfm/Ganymede",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "locate": "electron-webpack dev --locate"
  },
  "build": {
    "appId": "io.dfm.Ganymede",
    "productName": "Ganymede",
    "publish": [
      {
        "provider": "github"
      }
    ],
    "fileAssociations": {
      "ext": "ipynb",
      "name": "ipynb"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "files": [
      "app/*.js"
    ],
    "extraResources": [
      "ganymede.sh"
    ]
  },
  "dependencies": {
    "@types/glob": "^7.1.1",
    "@types/which": "^1.3.2",
    "@types/yargs": "^13.0.3",
    "electron-log": "^3.0.9",
    "glob": "^7.1.6",
    "source-map-support": "^0.5.16",
    "untildify": "^4.0.0",
    "which": "^2.0.1",
    "yargs": "^15.0.1"
  },
  "devDependencies": {
    "electron": "^7.1.1",
    "electron-builder": "^21.2.0",
    "electron-webpack": "^2.7.4",
    "electron-webpack-ts": "^3.2.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2"
  }
}